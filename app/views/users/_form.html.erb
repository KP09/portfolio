<%= simple_form_for(@user) do |f| %>
  <%= f.input :email %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :bio %>
  <%= f.input :profile_picture, as: :attachinary %>
  <%= f.input :cover_photo, as: :attachinary %>
  <%= f.submit value: "Save", class: "btn btn-primary" %>
<% end %>
